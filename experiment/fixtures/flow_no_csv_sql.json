{
  "title": "What would indicate good flow?",
  "tags": ["test", "fred / ned"],
  "train": {
    "dataset": {
      "fields": ["group","district","height","flow"]
    }
  },
  "ruleFields": ["group","district","height"],
  "aggregators": [
    {
      "name": "goodFlowMCC",
      "kind": "mcc",
      "arg": "flow > 60"
    }
  ],
  "goals": ["goodFlowAccuracy > 10"],
  "sortOrder": [
    {
      "aggregator": "goodFlowMCC",
      "direction": "descending"
    },
    {
      "aggregator": "numMatches",
      "direction": "descending"
    }
  ]
}
