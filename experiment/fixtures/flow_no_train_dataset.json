{
  "title": "What would indicate good flow?",
  "tags": ["test", "fred / ned"],
  "category": "testing",
  "test": {
    "dataset": {
      "csv": {
        "filename": "fixtures/flow.csv",
        "hasHeader": true,
        "separator":  ","
      },
      "fields": ["group","district","height","flow"]
    }
  },
  "ruleGeneration": {
    "fields": ["group","district","height"],
    "arithmetic": true
  },
  "aggregators": [
    {
      "name": "goodFlowMcc",
      "kind": "mcc",
      "arg": "flow > 60"
    }
  ],
  "goals": ["goodFlowMcc > 0"],
  "sortOrder": [
    {
      "aggregator": "goodFlowMcc",
      "direction": "descending"
    },
    {
      "aggregator": "numMatches",
      "direction": "descending"
    }
  ],
  "rules": [
    "height > 67",
    "height >= 129",
    "group == \"a\"",
    "flow <= 9.42",
    "district != \"northcal\" && group == \"b\""
  ]
}
